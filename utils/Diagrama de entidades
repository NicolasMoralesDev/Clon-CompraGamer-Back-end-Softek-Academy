https://dbdiagram.io/d/6560e2503be1495787af2b65

Table products {
  id integer [primary key]
  name string
  description string
  price integer
  category string
  brand string
  image_url string
  enabled boolean
}

Table users {
  id integer [primary key]
  username string
  password string
  role enum
  created_at timestamp
  address string
  phone integer
  enabled boolean
}

Table orders {
  id integer [primary key]
  user_id integer
  total integer
  created_at timestamp
}

Table order_list {
  id integer [primary key]
  ticket_id integer
  product_id integer
  quantity integer
}


Ref: order_list.ticket_id > orders.id // many-to-one
Ref: order_list.product_id > products.id // many-to-one
Ref: orders.user_id > users.id // many-to-one
